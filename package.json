{
  "name": "gamebuild-sdk-cli",
  "version": "1.0.0",
  "description": "GameBuild SDK Command Line Interface for game development and deployment",
  "main": "dist/index.js",
  "bin": {
    "gamebuild": "dist/cli.js",
    "gb": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "watch": "nodemon --exec ts-node src/cli.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build"
  },
  "keywords": [
    "gamebuild",
    "game-development",
    "cli",
    "sdk",
    "deployment",
    "build-tools"
  ],
  "author": "GameBuild Official",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.0",
    "inquirer": "^12.8.2",
    "ethers": "^6.8.1",
    "web3": "^4.3.0",
    "ipfs-http-client": "^60.0.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/inquirer": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
